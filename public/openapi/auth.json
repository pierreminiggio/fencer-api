{
    "openapi": "3.0.0",
    "info": {
      "title": "Auth",
      "version": "1.0.0"
    },
    "tags": [
      {
        "name": "Auth",
        "description": ""
      }
    ],
    "paths": {
      "/register": {
        "post": {
          "tags": [
            "Auth"
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/user_create"
                }
              }
            }
          },
          "responses": {
            "201": {},
            "400": {
              "$ref": "#/components/responses/BadRequest"
            }
          }
        }
      }
    },
    "components": {
      "securitySchemes": {
        "userAuth": {
          "type": "http",
          "scheme": "bearer"
        }
      },
      "responses": {
        "BadRequest": {
          "description": "Bad Request",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BadRequestError"
              }
            }
          }
        },
        "NotFound": {
          "description": "Not Found",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NotFoundError"
              }
            }
          }
        },
        "SuccessEmpty": {
          "description": "No content"
        },
        "Success": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Success"
              }
            }
          }
        }
      },
      "schemas": {
        "user_create": {
          "type": "object",
          "properties": {
            "login": {
              "type": "string",
              "example": "pseudo"
            },
            "email": {
              "type": "string",
              "example": "pseudo@gmail.com"
            },
            "password": {
              "type": "string",
              "example": "kjfgnd.kf12iFg"
            }
          }
        },
        "test": {
          "type": "object",
          "properties": {
            "param1": {
              "type": "string",
              "example": "param1"
            },
            "date": {
              "type": "string",
              "format": "date-time",
              "example": "2020-09-09T12:33:20+00:00"
            },
            "enum": {
              "type": "integer",
              "enum": [
                1,
                2,
                3
              ],
              "description": "enum"
            }
          }
        },
        "BadRequestError": {
          "type": "object",
          "required": [
            "error"
          ],
          "properties": {
            "error": {
              "type": "string",
              "enum": [
                "bad_content",
                "missing_parameter"
              ]
            },
            "message": {
              "$ref": "#/components/schemas/message"
            }
          }
        },
        "NotFoundError": {
          "type": "object",
          "required": [
            "error"
          ],
          "properties": {
            "error": {
              "type": "string",
              "enum": [
                "resource_not_found"
              ]
            }
          }
        },
        "Success": {
          "type": "object",
          "required": [
            "message"
          ],
          "properties": {
            "message": {
              "$ref": "#/components/schemas/message"
            }
          }
        },
        "message": {
          "type": "string",
          "example": "ceci est un message"
        }
      }
    }
  }
  